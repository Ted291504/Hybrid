local KeyGuardLibrary = loadstring(game:HttpGet("https://cdn.keyguardian.org/library/v1.0.0.lua"))()
local trueData = "6fcea905dc224035a098cc29bd559cea"
local falseData = "fc86e7ae09cc41578337a76d8585b0f5"

local KeySys = KeyGuardLibrary.Set({
	publicToken = "1ceea3083f0a4c46aa50013ba25226d7",
	privateToken = "4d87c0e28ff846fbbff9ff1a70c299eb",
	trueData = trueData,
	falseData = falseData,
})

local enableautoexec = clonefunction(client.enableautoexec);
setreadonly(client, false);
client.enableautoexec = nil
setreadonly(client, true);

if isfile("HybridKey.txt") and (KeyGuardLibrary.validateDefaultKey(readfile("HybridKey.txt")) == trueData or KeyGuardLibrary.validatePremiumKey("HybridKey.txt")) == trueData then
	loadstring(game:HttpGet("https://raw.githubusercontent.com/Ted291504/Hybrid/refs/heads/main/main%20ui",true))()
	enableautoexec()
else
	loadstring(game:HttpGet("https://raw.githubusercontent.com/Ted291504/Hybrid/refs/heads/main/UI/KeySystem%20UI",true))()
end
